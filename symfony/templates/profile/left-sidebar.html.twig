<div class="col-lg-3 col-md-4 pd-left-none no-pd">
    <div class="main-left-sidebar">
        <div class="user_profile">
            <div class="user-pro-img">
                {% if user.getUserProfilePictures().last() %}
                    <img src="{{ asset('/uploads/user/profiles/'~ user.getUserProfilePictures().last().document)|imagine_filter('user_profile_big') }}" alt="">
                {% else %}
                    <img src="https://via.placeholder.com/170x170" alt="">
                {% endif %}

                {% if app.user.id == user.id %}
                    <a href="#" class="profile-photo-open" title=""><i class="fa fa-camera"></i></a>
                {% endif %}

            </div><!--user-pro-img end-->
            <div class="user_pro_status">
                {% if app.user.id != user.id %}
                    <div class="relationship-button">
                        {% include 'profile/box/relationship-buttons.html.twig' %}
                    </div>
                {% else %}
                    <ul class="flw-hr">
                        <li>
                            <a href="#" title="" class="flww"><i class="la la-rocket"></i>
                                {% set count =  userRelationShip.followers|length %}
                                {% transchoice count with {'%count%': count} %}profile.count_followers{% endtranschoice %}
                            </a>
                        </li>
                        <li>
                            <a href="#" title="" class="hre"><i class="la la-users"></i>
                                {% set count =  userRelationShip.friends|length %}
                                {% transchoice count with {'%count%': count} %}profile.count_friends{% endtranschoice %}
                            </a>
                        </li>
                    </ul>
                {% endif %}
            </div><!--user_pro_status end-->
