<div class="profile-photo-box" id="profile-photo-box">
    <div class="profile-photo-edit">
        <h3>Profile picture</h3>
        <span>5000 character left</span>
        {{ form_start(formProfile) }}
        {{ form_row(formProfile.documentFile) }}
